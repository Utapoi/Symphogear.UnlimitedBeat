{
    "metadata": [
        {
            "src": [
                {
                    "src": "..",
                    "files": [
                        "Assets/**/*.cs"
                    ],
                    "exclude": [
                        "Assets/Plugins/**"
                    ]
                }
            ],
            "globalNamespaceId": "Global",
            "filter": "filterConfig.yml",
            "dest": "api"
        }
    ],
    "build": {
        "globalMetadata": {
            "_appName": "Symphogear: Unlimited Beat",
            "_appTitle": "Symphogear.UnlimitedBeat",
            "_appFooter": "Utapoi Ltd.<br/>Generated by <b>DocFX</b>",
            "_enableSearch": true,
            "_disableAffix": true,
            "_disableSideFilter": false,
            "_enableNewTab": true,
            "_disableContribution": false,
            "_disableBreadcrumb": false,
            "_gitContribute": {
              "repo": "https://github.com/Utapoi/Symphogear.UnlimitedBeat"
            },
            "_gitUrlPattern": "github"
        },
        "template": [ "default", "templates/singulinkfx", "plugins/memberpage.2.59.4/content", "plugins/memberpage-extras" ],
        "content": [
            {
                "files": [
                    "toc.yml",
                    "index.md"
                ]
            },
            {
                "src": "api",
                "files": [
                    "*.yml"
                ],
                "dest": "api"
            },
            {
                "src": "articles",
                "files": [
                    "toc.yml",
                    "*.md",
                    "**/*.md",
                    "**/*.yml"
                ],
                "dest": "articles"
            }
        ],
        "overwrite": [
            {
                "src": "..",
                "files": [
                    "Assets/**/*.md"
                ]
            }
        ],
        "resource": [
            {
                "files": [
                    "resources/**/*",
                    "Images/**/*"
                ]
            }
        ],
        "sitemap": {
            "baseUrl": "https://utapoi.github.io/SymphogearUnlimitedBeat",
            "changefreq": "weekly",
            "fileOptions": {
                "api/*": {
                    "changefreq": "daily"
                }
            }
        },
        "xref": [
            "https://utapoi.github.io/SymphogearUnlimitedBeat/UnityXrefMaps/2022.2/xrefmap.yml"
        ],
        "xrefService": [
            "https://xref.docs.microsoft.com/query?uid={uid}"
        ],
        "dest": "../_site",
        "postProcessors": [],
        "markdownEngineName": "markdig",
        "noLangKeyword": false,
        "keepFileLink": false,
        "cleanupCacheHistory": false,
        "disableGitFeatures": false
    }
}
